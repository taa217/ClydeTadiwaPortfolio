{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/dist/api/index.js" },
    { "source": "/(.*)", "destination": "/dist/public/index.html" }
  ]
}
