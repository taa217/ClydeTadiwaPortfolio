{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist/public",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/api/index.ts"
    },
    {
      "source": "/robots.txt",
      "destination": "/api/index.ts"
    },
    {
      "source": "/rss.xml",
      "destination": "/api/index.ts"
    },
    {
      "source": "/feed.json",
      "destination": "/api/index.ts"
    },
    {
      "source": "/blog/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/projects/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}